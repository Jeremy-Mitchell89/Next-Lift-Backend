enum Permission {
  ADMIN
  USER
  ITEMCREATE
  ITEMUPDATE
  ITEMDELETE
  PERMISSIONUPDATE
}

type User {
  id: ID! @unique
  name: String!
  email: String! @unique
  password: String!
  resetToken: String
  resetTokenExpiry: String
  permissions: [Permission!]!
  benchPress: Int @default(value: "100")
  squat: Int @default(value: "100")
  deadLift: Int @default(value: "100")
  press: Int @default(value: "100")
}
